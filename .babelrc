{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source",
        [
          "import",
          {
            "libraryName": "@popui/popui-rn",
            "libraryDirectory": "components"
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        [
          "import",
          {
            "libraryName": "@popui/popui-rn",
            "libraryDirectory": "components"
          }
        ]
      ]
    },
    "test": {
      "plugins": [
        [
          "import",
          {
            "libraryName": "@popui/popui-rn",
            "libraryDirectory": "components"
          }
        ]
      ]
    }
  }
}
